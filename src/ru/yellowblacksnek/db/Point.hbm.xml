<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ru.yellowblacksnek.beans.Point" table="points" catalog="postgres" optimistic-lock="version">
        <id name="id" type="int">
            <column name="id" />
            <generator class="increment" />
        </id>
        <property name="x" type="string">
            <column name="x" />
        </property>
        <property name="y" type="string">
            <column name="y" />
        </property>
        <property name="r" type="string">
            <column name="r" />
        </property>
        <property name="match" type="boolean">
            <column name="match" />
        </property>
    </class>
</hibernate-mapping>